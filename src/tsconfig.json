{
    "compilerOptions": {
        "module": "es2015",
        "target": "es3",
        "lib": ["es2017","dom"],
        "noImplicitAny": false,
        "sourceMap": false,
        "noResolve": false,
        "diagnostics": false,
        "removeComments": true,
        "skipDefaultLibCheck": true,
        "outDir": "../dist",
        "strict": true
    },
    "exclude": [
        "bin",
        "node_modules"
    ]
}
